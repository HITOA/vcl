#ifndef BUILTIN_TYPE
    #define BUILTIN_TYPE(id)
#endif
#ifndef SIGNED_TYPE
    #define SIGNED_TYPE(id) BUILTIN_TYPE(id)
#endif
#ifndef UNSIGNED_TYPE
    #define UNSIGNED_TYPE(id) BUILTIN_TYPE(id)
#endif
#ifndef FLOATING_TYPE
    #define FLOATING_TYPE(id) BUILTIN_TYPE(id)
#endif

BUILTIN_TYPE(Void)

UNSIGNED_TYPE(Bool)
UNSIGNED_TYPE(UInt8)
UNSIGNED_TYPE(UInt16)
UNSIGNED_TYPE(UInt32)
UNSIGNED_TYPE(UInt64)

SIGNED_TYPE(Int8)
SIGNED_TYPE(Int16)
SIGNED_TYPE(Int32)
SIGNED_TYPE(Int64)

FLOATING_TYPE(Float32)
FLOATING_TYPE(Float64)

#undef BUILTIN_TYPE
#undef SIGNED_TYPE
#undef UNSIGNED_TYPE
#undef FLOATING_TYPE
